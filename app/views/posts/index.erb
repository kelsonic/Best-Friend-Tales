<div class="row">
  <div class="col s12 center indego-text">
    <h2>Recent Posts</h2>
  </div>
</div>

<div class="row">

  <div class="col s12 m8 offset-m2 l8 offset-l2 center">
    <ul class="collapsible" data-collapsible="accordion">
      <% @posts.each do |post| %>
        <%= erb :"posts/_post", locals: {post: post} %>
      <% end %>

    </ul>
  </div>
</div>


